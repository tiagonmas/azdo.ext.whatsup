{
 	"manifestVersion": 1,
 	"id": "cse-follwing-feed",
 	"publisher": "TiagoAndradeeSilva",
 	"name": "What's Up?",
 	"description": "Recent updates from the work items you follow",
	"scopes": [ "vso.work"],
	 "categories": ["Azure Repos"],
	 "icons": {
		"default": "img/default-icon.png"
	  },
 	"targets": [
 		{
 			"id": "Microsoft.VisualStudio.Services"
 		}
 	],
 	"contributions": [
 		{
 			"id": "cse-follwing-feed-hub",
 			"type": "ms.vss-web.hub",
			"description": "Recent updates from the work items you follow",
 			"targets": [
 				"ms.vss-work-web.work-hub-group"
 			],
 			"properties": {
 				"name": "What's Up?",
				 "uri": "index.html",
				 "icon": "asset://img/default-icon.png"
 			}
 		}
 	],
 	"files": [
 		{
 			"path": "index.html",
 			"addressable": true
		 },
		 {
			"path": "feed.js",
			"addressable": true
		 }
		 ,
		 {
			"path": "main.js",
			"addressable": true
		 },
		 {
			"path": "azdo.css",
			"addressable": true
		},
		{
            "path": "img",
            "addressable": true
        },
 		{
 			"path": "node_modules/vss-web-extension-sdk/lib",
 			"addressable": true,
 			"packagePath": "lib"
 		}
 	]
 }